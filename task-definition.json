{
    "family": "parachain-monitor-task",
    "containerDefinitions": [
    {
        "name": "parachain-node-monitor-container",
        "image": "public.ecr.aws/n7o5v9f9/parachain-node-monitoring:latest",
        "portMappings":[
          {
            "containerPort": 9090,
            "hostPort": 9090,
            "protocol": "tcp"
          },
          {
            "containerPort": 9094,
            "hostPort": 9094,
            "protocol": "tcp"
          },
          {
            "containerPort": 9093,
            "hostPort": 9093,
            "protocol": "tcp"
          },
          {
            "containerPort": 9100,
            "hostPort": 9100,
            "protocol": "tcp"
          }
        ]
    }
    ],
    "requiresCompatibilities": [
    "FARGATE"
    ],
    "executionRoleArn" : "arn:aws:iam::316126673718:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "memory": "3 GB",
    "cpu": "512"
}
